(page "index.html"
      (:require [datascript.core :as d]
                [cognitect.transit :as t]))

(defc polidata nil)

(.ajax js/jQuery #js {:url "data.json"
                      :dataType "text"
                      :success (fn [res]
                                 (let [dt (t/read (t/reader :json) res)]
                                   (reset! polidata dt)))})

(cell= (println polidata))

(html
  (head
    (link :href "css/main.css" :rel "stylesheet"))
  (body
    (h1 "Hello, Hoplon!")))
